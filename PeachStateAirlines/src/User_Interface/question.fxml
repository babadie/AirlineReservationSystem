<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPaneQuestion" prefHeight="400.0" prefWidth="600.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="securityquestion.questioncontroller">
   <children>
      <Label fx:id="question" alignment="CENTER" contentDisplay="CENTER" layoutX="224.0" layoutY="146.0" text="SECURITY QUESTION" textFill="WHITE">
         <font>
            <Font name="Monospaced Regular" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="answerInput" layoutX="217.0" layoutY="187.0" />
      <Button fx:id="submit" layoutX="268.0" layoutY="259.0" mnemonicParsing="false" text="SUBMIT">
         <font>
            <Font name="Monospaced Regular" size="13.0" />
         </font>
      </Button>
      <Label fx:id="peachy" layoutX="188.0" layoutY="81.0" text="PEACHY AIRLINES" textFill="WHITE">
         <font>
            <Font name="Monospaced Regular" size="25.0" />
         </font>
      </Label>
      <Button fx:id="mainMenu" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="MAIN MENU">
         <font>
            <Font name="Monospaced Regular" size="11.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
